{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "es2017",
      "dom"
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "declaration": true
  },
  "include": ["src"],
  "paths": {
    "#app": [
      "./node_modules/nuxt3/dist/app/index"
    ]
  }
}